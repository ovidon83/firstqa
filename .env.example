# Server configuration
PORT=3000
NODE_ENV=development
SESSION_SECRET=your-session-secret

# GitHub App configuration
GITHUB_APP_ID=your-github-app-id
# Staging branch names for post-merge analysis (comma-separated, case-insensitive)
STAGING_BRANCHES=staging,stage,develop

# Optional: default base URL for /qa -testrun when -env= is not provided
TEST_AUTOMATION_BASE_URL=https://staging.example.com

# Product knowledge system (continuous codebase understanding)
ENABLE_KNOWLEDGE_SYNC=true
AUTO_INDEX_ON_INSTALL=true
INITIAL_ANALYSIS_BATCH_SIZE=50
FIRST_TIME_INDEX_REPO_CAP=5
GITHUB_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
Your GitHub App private key here
-----END RSA PRIVATE KEY-----"
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
